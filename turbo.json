{
  "baseBranch": "origin/main",
  "globalDependencies": ["patches/**", "yarn.lock"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "dist/**", ".chakraIcons/**"]
    },
    "test": {
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "export": {
      "dependsOn": ["build"]
    },
    "format": {
      "cache": false
    },
    "lint": {
      "dependsOn": ["^lint"]
    }
  }
}
